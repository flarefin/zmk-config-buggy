// Homerow Mods
#define hrml(k1,k2,k3,k4) &ht LCTRL k1  &ht LALT k2  &ht LSHFT k3  &ht LCMD k4
#define hrmr(k1,k2,k3,k4) &ht RCMD k1  &ht RSHFT k2  &ht RALT k3  &ht RCTRL k4

#define A1 0
#define A2 1
#define S1 2
#define S2 3
#define NUM 4
#define NAV1 5
#define NAV2 6
#define BT 7
#define GM1 8
#define GM2 9
#define CB1 10
#define CB2 11

